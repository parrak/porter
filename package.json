{
  "name": "flight-search-app",
  "version": "1.0.0",
  "description": "Flight search application using Amadeus API",
  "main": "api/index.js",
  "scripts": {
    "dev": "vercel dev",
    "build": "vercel build",
    "start": "vercel dev",
    "test:unit": "jest tests/unit/",
    "test:integration": "jest tests/integration/",
    "test:full": "jest",
    "test:quick": "jest --passWithNoTests",
    "test:debug": "jest --verbose --detectOpenHandles",
    "validate": "node validate-js.js",
    "validate:js": "node validate-js.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "ci:test": "npm run test:unit && npm run validate:js",
    "ci:full": "npm run test:full && npm run validate:js && npm run security:audit",
    "pre-commit": "npm run validate:js && npm run test:unit"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "amadeus": "^8.1.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^29.7.0",
    "openai": "^5.12.2",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "amadeus",
    "flight",
    "travel",
    "api"
  ],
  "author": "",
  "license": "ISC"
}
