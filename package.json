{
  "name": "flight-search-app",
  "version": "1.0.0",
  "description": "Flight search application using Amadeus API",
  "main": "api/index.js",
  "scripts": {
    "start": "node web-api.js",
    "dev": "node web-api.js",
    "build": "echo 'No build step required'",
    "vercel-build": "echo 'No build step required'",
    "postinstall": "echo 'Static OAuth URLs - no dynamic updates needed'",
    "update-oauth": "node scripts/update-oauth-urls.js",
    "update-current": "node scripts/update-current-oauth.js porter-preview.vercel.app",
    "update-env": "node scripts/update-env-vars.js",
    "setup-db": "node setup-database.js",
    "setup-neon-db": "node setup-neon-postgres.js",
    "test-db": "node test-database.js",
    "demo-profile": "node demo-user-profile.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:full": "node scripts/run-tests.js",
    "test:quick": "jest --passWithNoTests --verbose",
    "test:debug": "jest --detectOpenHandles --forceExit --verbose",
    "lint": "echo 'Linting not configured yet'",
    "lint:fix": "echo 'Linting not configured yet'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "ci:test": "npm run test:ci && npm run security:audit",
    "ci:full": "npm run test:full && npm run security:audit",
    "run": "node main.js"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "amadeus": "^8.1.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^29.7.0",
    "openai": "^5.12.2",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "amadeus",
    "flight",
    "travel",
    "api"
  ],
  "author": "",
  "license": "ISC"
}
