{
  "buildCommand": "echo 'Static OAuth URLs - no dynamic updates needed'",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/privacy-policy",
      "destination": "/api/privacy-policy.js"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/openapi.js"
    }
  ]
}
