{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/privacy-policy",
      "dest": "/api/privacy-policy.js"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/openapi.js"
    }
  ]
}
