{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/openapi.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/openapi",
      "dest": "/api/openapi.js"
    },
    {
      "src": "/api/chatgpt",
      "dest": "/api/chatgpt.js"
    },
    {
      "src": "/api/search-flights",
      "dest": "/api/search-flights.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
